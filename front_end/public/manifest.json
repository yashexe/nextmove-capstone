{
  "manifest_version": 3,
  "name": "NextMove",
  "description": "Email Clutter Management",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "windows"
  ],
  "host_permissions": [
    "http://127.0.0.1/*",
    "https://mail.google.com/*"
  ],
  "icons": {
    "16": "icon_final_16.png",
    "48": "icon_final_48.png",
    "128": "icon_final_128.png"
  }
}