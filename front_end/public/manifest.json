{
  "manifest_version": 3,
  "name": "NextMove",
  "description": "Email Clutter Management",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "windows"
  ],
  "host_permissions": [
    "http://127.0.0.1/*",
    "https://mail.google.com/*"
  ]
}
